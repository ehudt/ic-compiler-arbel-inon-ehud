class Point {
	int x;
	int y;
	
	int getX()
	{
		return x;
	}
	
	int getY()
	{
		return y;
	}
	
	void setX(int x)
	{
		this.x=x;
	}
	
	void setY(int y)
	{
		this.y=y;
	}
	
	void printPoint()
	{
		Library.print("(x=");
		Library.printi(x);
		Library.print(",y=");
		Library.printi(y);
		Library.print(")");
		Library.println("");
		
	}
}

class PolarPoint extends Point
{
	int rad;
}

class Main{
	static void main(string[] args) 
	{
		
		Point p = new PolarPoint();
		p.setX(5);
		p.setY(6);	
		p.printPoint();
	}

}